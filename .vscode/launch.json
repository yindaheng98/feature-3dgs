{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "train",
            "type": "debugpy",
            "request": "launch",
            "module": "feature_3dgs.train",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/truck",
                "-d",
                "output/truck",
                "-i",
                "30000",
                "--mode",
                "densify",
                "--empty_cache_every_step",
                "-odensify_grad_threshold=0.0001",
            ]
        },
        {
            "name": "render",
            "type": "debugpy",
            "request": "launch",
            "module": "feature_3dgs.render",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-s",
                "data/truck",
                "-d",
                "output/truck",
                "-i",
                "30000",
            ]
        },
        {
            "name": "show",
            "type": "debugpy",
            "request": "launch",
            "module": "feature_3dgs.dataset.show",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "-n",
                "dinov3_vits16",
                "-s",
                "data/flame_steak/frame1",
                "-d",
                "output/flame_steak-semantic/frame1",
            ]
        },
    ]
}